{
  "extends": [
    "github>entur/ror-renovate-config",
    "group:allNonMajor",
    "schedule:weekly"
  ],
  "npmrcMerge": true,
  "timezone": "Europe/Oslo",
  "packageRules": [
    {
      "groupName": "vite",
      "matchPackageNames": [
        "vite-*",
        "@vitejs/*",
        "vitest",
        "@vitest/*"
      ]
    }
  ]

}
